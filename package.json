{
  "name": "@plasmo-corp/mwu",
  "version": "2.2.0",
  "description": "Mozilla Webstore Upload",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.14"
  },
  "scripts": {
    "dev": "esbuild src/index.ts --format=esm --bundle --watch --sourcemap --platform=node --outdir=dist --external:got --external:jsonwebtoken",
    "build": "tsup src/index.ts --format esm --minify --dts-resolve",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf dist",
    "prepublishOnly": "run-s clean build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plasmo-corp/mwu.git"
  },
  "author": "Plasmo Corp. <support@plasmo.com>",
  "keywords": [
    "browser-extensions",
    "firefox",
    "webext",
    "browser-addons",
    "browser-plugins"
  ],
  "license": "MIT",
  "dependencies": {
    "got": "12.0.2",
    "jsonwebtoken": "8.5.1",
    "form-data-encoder": "1.7.1",
    "formdata-node": "4.3.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.8",
    "@jest/globals": "27.5.1",
    "@plasmo-corp/rps": "1.2.1",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "17.0.23",
    "esbuild": "0.14.27",
    "cross-env": "7.0.3",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.4",
    "tsup": "5.12.1",
    "typescript": "4.6.3"
  }
}
